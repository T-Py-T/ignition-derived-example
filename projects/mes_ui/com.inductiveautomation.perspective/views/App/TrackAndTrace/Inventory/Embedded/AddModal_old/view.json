                        "binding": {
                          "config": {
                            "expression": "runScript(\"mes.trace.sp.getInventories\", 0, 1)"
                          },
                          "transforms": [
                            {
                              "code": "\toptions \u003d []\n\tpydata \u003d system.dataset.toPyDataSet(value)\n\t\n\tfor row in pydata:\n\t\toptions.append({\n\t\t\t\"label\": row[\"Name\"],\n\t\t\t\"value\": row[\"ID\"]\n\t\t})\n\t\t\n\treturn options\n\t\t\t\n\t",
                              "type": "script"
                            }
                          ],
                          "type": "expr"
                        }
                      },
                      "props.options": {
                        "binding": {
                          "config": {
                            "path": "this.custom.options"
                          },
                          "type": "property"
                        }
                      },
                      "props.value": {
                        "onChange": {
                          "enabled": null,
                          "script": "\tpayload \u003d currentValue.getValue()\n\tif previousValue !\u003d currentValue:\n\t\tsystem.perspective.sendMessage(\"inventorySelected\", payload\u003dpayload)\n\t\t"
                        }
                      }
                    },
