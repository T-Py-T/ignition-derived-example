                }
              ],
              "type": "property"
            }
          },
          "props.params.disabledPopup.message": {
            "binding": {
              "config": {
                "expression": "\u0027There are \u0027 + {../oeeDowntimeTable.custom.badReasonCount} + \u0027 bad reason(s) codes left on the downtime table\u0027"
              },
              "type": "expr"
            }
          },
          "props.params.enabled": {
            "binding": {
              "config": {
                "expression": "{../oeeDowntimeTable.custom.badReasonCount} \u003d 0"
              },
              "enabled": false,
              "type": "expr"
            },
            "onChange": {
              "enabled": false,
              "script": "\tfrom time import sleep\n\tsleep(2)\n\tid \u003d \u0027downtimeTable\u0027\n\tif self.props.params.enabled \u003d\u003d True: #and self.getSibling(\"oeeDowntimeTable\").props.data \u003d\u003d 0:\n\t\tsystem.perspective.print(\u0027Close Button Enabled \u003d True, pop up close\u0027)\n\t\tsystem.perspective.closePopup(id)\n\t\tsystem.perspective.print(\u0027enabled?: \u0027 + str(self.props.params.enabled))\n\telif self.props.params.enabled \u003d\u003d False:\n\t\tsystem.perspective.print(\u0027Close Button Enabled \u003d False, pop up stays open\u0027)\n\t\tsystem.perspective.print(\u0027enabled?: \u0027 + str(self.props.params.enabled))\n\telse:\n\t\tsystem.perspective.print(\u0027error\u0027)\n\t\tsystem.perspective.print(self.props.params.enabled)\n\t\t#system.perspective.closePopup(id)\n\t\n\t#if self.custom.close \u003d\u003d True:\n\t\t#id \u003d \u0027downtimeTable\u0027\n\t\t#system.perspective.closePopup(id)\n\t\n#\t\tsystem.perspective.print(str(origin))"
