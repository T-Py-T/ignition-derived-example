{
  "custom": {
    "Hourly": 1860,
    "OperatingTime": "0:04:00",
    "PlannedProdTime": 78,
    "RuntimePlanned": "0:35:00",
    "Target": 1,
    "UnplannedDowntime": "value",
    "WOQuantity": 145000
  },
  "params": {
    "EqPath": "",
    "State": ""
  },
  "propConfig": {
    "custom.Hourly": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "1": "{view.params.EqPath}"
          },
          "tagPath": "{1}/jobs/currentJob/standardRate"
        },
        "transforms": [
          {
            "expression": "{value} * 60",
            "type": "expression"
          }
        ],
        "type": "tag"
      },
      "persistent": true
    },
    "custom.OperatingTime": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "1": "{view.params.EqPath}"
          },
          "tagPath": "{1}/calculatedValues/ShiftRuntime"
        },
        "transforms": [
          {
            "expression": "runScript(\u0027gpa.frontEnd.localUI.convert\u0027, 5000, \u0027min\u0027, {value})",
            "type": "expression"
          }
        ],
        "type": "tag"
      },
      "persistent": true
    },
    "custom.PlannedProdTime": {
      "binding": {
        "config": {
          "path": "view.custom.WOQuantity"
        },
        "transforms": [
          {
            "expression": "round({value} / {view.custom.Hourly})",
            "type": "expression"
          }
        ],
        "type": "property"
      },
      "persistent": true
    },
    "custom.RuntimePlanned": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "1": "{view.params.EqPath}"
          },
          "tagPath": "{1}/calculatedValues/ShiftRuntimePlanned"
        },
        "transforms": [
          {
            "expression": "runScript(\u0027gpa.frontEnd.localUI.convert\u0027, 5000, \u0027min\u0027, {value})",
            "type": "expression"
          }
        ],
        "type": "tag"
      },
      "persistent": true
    },
    "custom.Target": {
      "persistent": true
    },
    "custom.UnplannedDowntime": {
      "persistent": true
    },
    "custom.WOQuantity": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "1": "{view.params.EqPath}"
          },
          "tagPath": "{1}/jobs/currentJob/workOrderQuantity"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "params.EqPath": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.State": {
      "binding": {
        "config": {
          "path": "view.custom.Percent"
        },
        "enabled": false,
        "transforms": [
          {
            "expression": "runScript(\u0027gpa.frontEnd.localUI.getRateStatus\u0027, 0, {value})\r\n",
            "type": "expression"
          }
        ],
        "type": "property"
      },
      "paramDirection": "output",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 76,
      "width": 288
    }
  },
  "root": {
    "children": [
      {
        "children": [
          {
            "meta": {
              "name": "Value"
            },
            "position": {
              "shrink": 0
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "path": "view.params.Name"
                  },
                  "type": "property"
                }
              }
            },
            "props": {
              "style": {
                "classes": "Cards/Common/CardTitle"
              }
            },
            "type": "ia.display.label"
          }
        ],
        "meta": {
          "name": "Name"
        },
        "position": {
          "basis": "20px",
          "display": false,
          "shrink": 0
        },
        "props": {
          "style": {
            "classes": "Cards/Common/CardTitleBorder",
            "overflow": "hidden"
          },
          "wrap": "wrap"
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "meta": {
              "name": "Label"
            },
            "position": {
              "grow": 1
            },
            "props": {
              "style": {
                "classes": "Cards/Common/CardText"
              },
              "text": "Target"
            },
            "type": "ia.display.label"
          },
          {
            "meta": {
              "name": "Value"
            },
            "position": {
              "basis": "75px",
              "grow": 1,
              "shrink": 0
            },
            "propConfig": {
              "props.style.backgroundColor": {
                "binding": {
                  "config": {
                    "path": "view.params.State"
                  },
                  "enabled": false,
                  "transforms": [
                    {
                      "fallback": "",
                      "inputType": "scalar",
                      "mappings": [
                        {
                          "input": "fail",
                          "output": "--error"
                        }
                      ],
                      "outputType": "scalar",
                      "type": "map"
                    }
                  ],
                  "type": "property"
                }
              },
              "props.text": {
                "binding": {
                  "config": {
                    "path": "view.custom.Target"
                  },
                  "transforms": [
                    {
                      "formatType": "numeric",
                      "formatValue": "percent",
                      "type": "format"
                    }
                  ],
                  "type": "property"
                }
              }
            },
            "props": {
              "style": {
                "backgroundColor": "",
                "classes": "Cards/Common/CardValue"
              },
              "textStyle": {
                "fontWeight": "bold",
                "textAlign": "right"
              }
            },
            "type": "ia.display.label"
          }
        ],
        "meta": {
          "name": "Target"
        },
        "position": {
          "basis": "20px",
          "display": false,
          "shrink": 0
        },
        "props": {
          "style": {
            "marginBottom": "2px",
            "overflow": "hidden"
          },
          "wrap": "wrap"
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "meta": {
              "name": "Label"
            },
            "position": {
              "grow": 1
            },
            "props": {
              "style": {
                "classes": "Cards/Common/CardText"
              },
              "text": "Operating Time"
            },
            "type": "ia.display.label"
          },
          {
            "meta": {
              "name": "Value"
            },
            "position": {
              "grow": 1,
              "shrink": 0
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "path": "view.custom.OperatingTime"
                  },
                  "type": "property"
                }
              }
            },
            "props": {
              "style": {
                "classes": "Cards/Common/Boolean"
              },
              "textStyle": {
                "fontWeight": "bold",
                "textAlign": "right"
              }
            },
            "type": "ia.display.label"
          }
        ],
        "meta": {
          "name": "OperatingTime"
        },
        "position": {
          "basis": "20px",
          "shrink": 0
        },
        "props": {
          "style": {
            "overflow": "hidden"
          },
          "wrap": "wrap"
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "meta": {
              "name": "Label"
            },
            "position": {
              "grow": 1
            },
            "props": {
              "style": {
                "classes": "Cards/Common/CardText"
              },
              "text": "Planned Production Time"
            },
            "type": "ia.display.label"
          },
          {
            "meta": {
              "name": "Value"
            },
            "position": {
              "shrink": 0
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "path": "view.custom.RuntimePlanned"
                  },
                  "type": "property"
                }
              }
            },
            "props": {
              "style": {
                "classes": "Cards/Common/CardValue"
              },
              "textStyle": {
                "textTransform": "lowercase"
              }
            },
            "type": "ia.display.label"
          }
        ],
        "meta": {
          "name": "PlannedProdTime"
        },
        "position": {
          "basis": "20px",
          "shrink": 0
        },
        "props": {
          "style": {
            "classes": "TextFields/TextFieldAlternate",
            "overflow": "hidden"
          },
          "wrap": "wrap"
        },
        "type": "ia.container.flex"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "classes": "Global/MarginAll"
      }
    },
    "type": "ia.container.flex"
  }
}