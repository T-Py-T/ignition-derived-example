{
  "custom": {
    "MinutesRemaining": 0,
    "NextOrder": "",
    "Order": "",
    "TimeRemaining": 374
  },
  "params": {
    "EqPath": ""
  },
  "propConfig": {
    "custom.MinutesRemaining": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "1": "{view.params.EqPath}"
          },
          "tagPath": "{1}/jobs/currentJob/minutesRemaining"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "custom.NextOrder": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "1": "{view.params.EqPath}"
          },
          "tagPath": "{1}/jobs/nextJob/workOrder"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "custom.Order": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "1": "{view.params.EqPath}"
          },
          "tagPath": "{1}/jobs/currentJob/workOrder"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "params.EqPath": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 164,
      "width": 517
    }
  },
  "root": {
    "children": [
      {
        "children": [
          {
            "meta": {
              "name": "Title"
            },
            "props": {
              "style": {
                "classes": "Global/MarginRight Text/NormalText"
              },
              "text": "Planned Order:"
            },
            "type": "ia.display.label"
          },
          {
            "meta": {
              "name": "OrderNumber"
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "path": "view.custom.Order"
                  },
                  "transforms": [
                    {
                      "expression": "if({value} !\u003d \"\", {value}, \"No order found\")",
                      "type": "expression"
                    }
                  ],
                  "type": "property"
                }
              }
            },
            "props": {
              "style": {
                "classes": "Text/NormalText"
              },
              "textStyle": {
                "fontWeight": "bold"
              }
            },
            "type": "ia.display.label"
          }
        ],
        "meta": {
          "name": "CurrentPlannedOrder"
        },
        "position": {
          "grow": 1
        },
        "props": {
          "style": {
            "classes": "Cards/Common/CardTitleBorder Global/MarginBottom",
            "overflow": "hidden"
          }
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "meta": {
              "name": "Title"
            },
            "props": {
              "alignVertical": "top",
              "style": {
                "classes": "Global/MarginRight Text/NormalText"
              },
              "text": "Current Run:"
            },
            "type": "ia.display.label"
          },
          {
            "children": [
              {
                "meta": {
                  "name": "RunInfoView"
                },
                "position": {
                  "grow": 1
                },
                "propConfig": {
                  "props.params.EqPath": {
                    "binding": {
                      "config": {
                        "path": "view.params.EqPath"
                      },
                      "type": "property"
                    }
                  }
                },
                "props": {
                  "path": "App/OEE/Operations/LineDashboard/Embedded/RunInformation",
                  "useDefaultViewHeight": true
                },
                "type": "ia.display.view"
              }
            ],
            "meta": {
              "name": "RunInformation"
            },
            "position": {
              "grow": 1
            },
            "propConfig": {
              "position.display": {
                "binding": {
                  "config": {
                    "path": "view.custom.Order"
                  },
                  "transforms": [
                    {
                      "expression": "if({value} !\u003d \"\", true, false) ",
                      "type": "expression"
                    }
                  ],
                  "type": "property"
                }
              }
            },
            "props": {
              "alignContent": "space-around",
              "alignItems": "center",
              "style": {
                "overflow": "hidden"
              }
            },
            "type": "ia.container.flex"
          }
        ],
        "meta": {
          "name": "CurrentRun"
        },
        "position": {
          "grow": 1
        },
        "props": {
          "direction": "column",
          "style": {
            "classes": "Cards/Common/CardTitleBorder Global/MarginBottom",
            "overflow": "hidden"
          }
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "meta": {
              "name": "Title"
            },
            "props": {
              "style": {
                "classes": "Global/MarginRight Text/NormalText"
              },
              "text": "Projected End of Order:"
            },
            "type": "ia.display.label"
          },
          {
            "meta": {
              "name": "dateFinished"
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "path": "view.custom.MinutesRemaining"
                  },
                  "transforms": [
                    {
                      "expression": "dateFormat(addMinutes(now(60000),{value}), \"yyyy-MM-dd HH:mm:ss aa\") ",
                      "type": "expression"
                    }
                  ],
                  "type": "property"
                }
              }
            },
            "props": {
              "style": {
                "classes": "Text/NormalText"
              },
              "textStyle": {
                "fontWeight": "bold"
              }
            },
            "type": "ia.display.label"
          }
        ],
        "meta": {
          "name": "TimeUntilChangeover"
        },
        "position": {
          "basis": "asuto",
          "grow": 1
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "path": "view.custom.Order"
              },
              "transforms": [
                {
                  "expression": "if({value} !\u003d \"\", true, false) ",
                  "type": "expression"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "overflow": "hidden"
          }
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "meta": {
              "name": "Title"
            },
            "props": {
              "style": {
                "classes": "Global/MarginRight Text/NormalText"
              },
              "text": "Next Planned Order:"
            },
            "type": "ia.display.label"
          },
          {
            "meta": {
              "name": "OrderNumber"
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "path": "view.custom.NextOrder"
                  },
                  "transforms": [
                    {
                      "expression": "if({value} !\u003d \"\", {value}, \"No order found\")",
                      "type": "expression"
                    }
                  ],
                  "type": "property"
                }
              }
            },
            "props": {
              "style": {
                "classes": "Text/NormalText"
              },
              "textStyle": {
                "fontWeight": "bold"
              }
            },
            "type": "ia.display.label"
          }
        ],
        "meta": {
          "name": "NextOrder"
        },
        "position": {
          "basis": "asuto",
          "grow": 1
        },
        "props": {
          "style": {
            "overflow": "hidden"
          }
        },
        "type": "ia.container.flex"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column"
    },
    "type": "ia.container.flex"
  }
}