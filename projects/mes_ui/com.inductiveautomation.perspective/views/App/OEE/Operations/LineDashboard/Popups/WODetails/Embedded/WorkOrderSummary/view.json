    "custom.material": {
      "binding": {
        "config": {
          "path": "view.custom.plannedOrderData.materialProduced[0].definitionId"
        },
        "transforms": [
          {
            "code": "\tfilter \u003d system.mes.object.filter.createFilter()\n\tfilter.setMESObjectTypeName(\u0027MaterialDef\u0027)\n\tfilter.setMESObjectNamePattern(\u0027*\u0027 + str(value))\n\t\n\tlist \u003d system.mes.searchMESObjects(filter)\n\tmatReturn \u003d {}\n\tif list.size() \u003e 0:\n\t\tmatObj \u003d list[0].getMESObject()\n\t\tprops \u003d matObj.getCustomPropertiesFull()\n\t\tmatReturn[\u0027definitionId\u0027] \u003d value\n\t\tfor prop in props:\n\t\t\tmatReturn[prop] \u003d props[prop][1]\n\treturn matReturn",
            "type": "script"
          }
        ],
        "type": "property"
      },
      "persistent": true
    },
    "custom.plannedOrderData": {
      "binding": {
        "config": {
          "path": "view.params.plannedOrder"
        },
        "transforms": [
          {
            "code": "\treturn mes.schedule.sap.getOrderDetails(value) ",
            "type": "script"
          }
        ],
        "type": "property"
      },
      "persistent": true
    },
