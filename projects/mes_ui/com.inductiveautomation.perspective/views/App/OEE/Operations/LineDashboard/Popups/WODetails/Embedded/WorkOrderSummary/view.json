      "binding": {
        "config": {
          "expression": "numberFormat(floor({view.custom.RunData}[\u0027RemainingHours\u0027]/ 60),\"#0\") + \u0027h\u0027\r\n+\":\"+numberFormat(60*(({view.custom.RunData}[\u0027RemainingHours\u0027] / 60)-floor({view.custom.RunData}[\u0027RemainingHours\u0027] / 60)),\"00\") + \u0027m\u0027\r\n"
