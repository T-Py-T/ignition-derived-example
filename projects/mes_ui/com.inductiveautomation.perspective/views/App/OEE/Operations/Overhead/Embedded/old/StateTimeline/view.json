        "position": {
          "grow": 1
        },
        "propConfig": {
          "custom.DS": {
            "binding": {
              "config": {
                "expression": "{this.custom.OriginalDS}"
              },
              "transforms": [
                {
                  "code": "\tarray \u003d []\n\tline \u003d self.custom.LineName\n\tpyDS \u003d system.dataset.toPyDataSet(value)\n\tfor row in pyDS:\n\t\tnewDict \u003d {}\n\t\tnewDict[\u0027START\u0027] \u003d row[\u0027StartDateTime\u0027]\n\t\tnewDict[\u0027END\u0027] \u003d row[\u0027EndDateTime\u0027]\n\t\tnewDict[\u0027STATUS\u0027] \u003d row[\u0027Status\u0027]\n\t\tnewDict[\u0027DURATION\u0027] \u003d row[\u0027Duration\u0027]\n\t\tnewDict[\u0027COLOR\u0027] \u003d row[\u0027COLOR\u0027]\n\t\tnewDict[\u0027LINE\u0027] \u003d line # write the last vin to this DS\n\t\tarray.append(newDict)\n\treturn array",
                  "type": "script"
                }
              ],
              "type": "expr"
            }
          },
          "custom.LineName": {
            "binding": {
              "config": {
                "path": "view.params.TagPath"
              },
              "transforms": [
                {
                  "expression": "substring({value},lastIndexOf({value},\u0027/\u0027)+1)",
                  "type": "expression"
                }
              ],
              "type": "property"
            }
          },
          "custom.OriginalDS": {
            "binding": {
              "config": {
                "fallbackDelay": 2.5,
                "mode": "indirect",
                "references": {
                  "1": "{view.params.TagProvider}",
                  "2": "{view.params.TagPath}"
                },
                "tagPath": "{1}{2}/calculatedValues/shiftDowntimeEvents"
              },
              "type": "tag"
            }
          },
          "props.dataSources.Level1Data": {
            "binding": {
              "config": {
                "expression": "{this.custom.DS}"
              },
              "type": "expr"
            }
          },
          "props.xAxes[0].date.break.endDate": {
            "binding": {
              "config": {
                "expression": "toStr(maxDate({this.custom.OriginalDS},1))"
              },
              "type": "expr"
            }
          },
          "props.xAxes[0].date.break.startDate": {
            "binding": {
              "config": {
                "expression": "toStr(minDate({this.custom.OriginalDS},0))"
              },
              "type": "expr"
            }
          }
        },
