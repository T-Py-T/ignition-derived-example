    "params.RollUps": {
      "binding": {
        "config": {
          "path": "view.custom.Data"
        },
        "transforms": [
          {
            "code": "\tlogger \u003d system.util.getLogger(\u0027stateTimeLineView\u0027)\n\t\n\ttry:\n\t\tjson_data \u003d gpa.data.datasetToJson(value)\n\t\tstate_rollup \u003d {}\n\t\t\n\t\tfor entry in json_data:\n\t\t    status \u003d entry[\u0027Status\u0027]\n\t\t    duration \u003d entry[\u0027Duration\u0027]\n\t\t    color \u003d entry.get(\u0027COLOR\u0027, \u0027\u0027)  # Assuming you have a \u0027Color\u0027 key, if not this will default to \u0027\u0027\n\t\t\n\t\t    if status in state_rollup:\n\t\t        state_rollup[status][\u0027duration\u0027] +\u003d duration\n\t\t    else:\n\t\t        state_rollup[status] \u003d {\n\t\t            \u0027status\u0027: status,\n\t\t            \u0027duration\u0027: duration,\n\t\t            \u0027color\u0027: color\n\t\t        }\n\t\t\n\t\treturn list(state_rollup.values())\n\t    \n\texcept Exception as e:\n\t    logger.warn(str(e))",
            "type": "script"
          }
        ],
        "type": "property"
      },
      "paramDirection": "output",
      "persistent": true
    }
  },
