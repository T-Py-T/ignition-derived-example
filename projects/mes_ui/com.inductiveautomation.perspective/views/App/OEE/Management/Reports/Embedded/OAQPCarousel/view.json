{
  "custom": {
    "ActivePane": 0,
    "Title": null
  },
  "params": {
    "Areas": [],
    "IsReport": true,
    "ReportData": "value",
    "State": "",
    "Thickness": 5,
    "Title": null
  },
  "propConfig": {
    "custom.ActivePane": {
      "binding": {
        "config": {
          "bidirectional": true,
          "path": "/root/Carousel.props.activePane"
        },
        "type": "property"
      },
      "persistent": true
    },
    "custom.Title": {
      "binding": {
        "config": {
          "path": "view.params.Areas"
        },
        "transforms": [
          {
            "expression": "{value}[{view.custom.ActivePane}][\u0027viewParams\u0027][\u0027AreaName\u0027]",
            "type": "expression"
          }
        ],
        "type": "property"
      },
      "persistent": true
    },
    "params.Areas": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.IsReport": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.ReportData": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.State": {
      "binding": {
        "config": {
          "path": "view.custom.ActivePane"
        },
        "enabled": false,
        "transforms": [
          {
            "code": "\ttry:\n\t\treturn self.getChild(\"root/Carousel\").props.views[value].viewParams.State\n\texcept Exception as Argument:\n\t\tsystem.perspective.print(Exception)",
            "type": "script"
          }
        ],
        "type": "property"
      },
      "paramDirection": "inout",
      "persistent": true
    },
    "params.Thickness": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.Title": {
      "binding": {
        "config": {
          "path": "view.custom.Title"
        },
        "type": "property"
      },
      "paramDirection": "output",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 208,
      "width": 560
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "basis": "32px",
          "display": false
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.custom.Title"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "Global/MarginAllLeft"
          },
          "textStyle": {
            "classes": "Cards/Common/CardTitle"
          }
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "Carousel"
        },
        "position": {
          "basis": "200px"
        },
        "propConfig": {
          "props.views": {
            "binding": {
              "config": {
                "path": "view.params.Areas"
              },
              "type": "property"
            }
          },
          "props.views[0].viewParams.Thickness": {
            "binding": {
              "config": {
                "path": "view.params.Thickness"
              },
              "type": "property"
            }
          }
        },
        "type": "ia.display.carousel"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column"
    },
    "type": "ia.container.flex"
  }
}