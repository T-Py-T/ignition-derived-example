  "propConfig": {
    "custom.DATAVALIDATION": {
      "persistent": true
    },
    "custom.DATAVALIDATION.DataType": {
      "binding": {
        "config": {
          "path": "view.custom.Datatype"
        },
        "type": "property"
      }
    },
    "custom.DATAVALIDATION.LimitsEntered": {
      "binding": {
        "config": {
          "expression": "If({/root/Limits/LimitsHiHi/HiHiValue.props.value} !\u003d Null \u0026\u0026 {/root/Limits/LimitsLoLo/LoLoValue.props.value} !\u003d Null\r\n\u0026\u0026 {/root/Limits/LimitsHi/HiValue.props.value} !\u003d Null\r\n\u0026\u0026 {/root/Limits/LimitsLo/LoValue.props.value} !\u003d Null, true, false)"
        },
        "type": "expr"
      }
    },
    "custom.DATAVALIDATION.LimitsRange": {
      "binding": {
        "config": {
          "path": "view.custom.DATAVALIDATION.LimitsEntered"
        },
        "transforms": [
          {
            "code": "\tif value \u003d\u003d True:\n\t\tHiHi \u003d self.custom.HiHi\n\t\tHi \u003d self.custom.Hi\n\t\tLo \u003d self.custom.Lo\n\t\tLoLo \u003d self.custom.LoLo\n\t\n\t    # Check if HiHi is the highest value\n\t\tif HiHi \u003e\u003d Hi and HiHi \u003e\u003d Lo and HiHi \u003e\u003d LoLo:\n\t\t\t# Check if Hi is the second greatest value but not greater than HiHi\n\t\t\tif Hi \u003e\u003d Lo and Hi \u003e\u003d LoLo and Hi \u003c\u003d HiHi:\n\t\t\t\t# Check if Lo is the third value but not lower than LoLo\n\t\t\t\tif Lo \u003e\u003d LoLo:\n\t\t\t\t\t# Check if LoLo is the lowest value and not higher than any of the above values\n\t\t\t\t\tif LoLo \u003c\u003d Hi and LoLo \u003c\u003d HiHi and LoLo \u003c\u003d Lo:\n\t\t\t\t\t    return True\n\t\treturn False\n\treturn False",
            "type": "script"
          }
        ],
        "type": "property"
      }
    },
    "custom.DATAVALIDATION.NameEntered": {
      "binding": {
        "config": {
          "expression": "if(len({view.custom.Name}) \u003e 0, true, false)"
        },
        "type": "expr"
      }
    },
    "custom.DATAVALIDATION.Pass": {
      "binding": {
        "config": {
          "expression": "if({view.custom.DATAVALIDATION.LimitsRange} \u0026\u0026 {view.custom.DATAVALIDATION.LimitsEntered} \u0026\u0026 {view.custom.DATAVALIDATION.NameEntered},\r\n true, if({view.custom.DATAVALIDATION.DataType} \u003d 3||{view.custom.DATAVALIDATION.DataType} \u003d 4, true, false))"
        },
        "type": "expr"
      }
    },
    "custom.Datatype": {
      "binding": {
        "config": {
          "path": "/root/Metadata/MetadataDatatype/Datatype.props.value"
        },
        "type": "property"
      },
      "persistent": true
    },
    "custom.HasLimits": {
      "binding": {
        "config": {
          "expression": "{/root/Metadata/MetadataDatatype/Datatype.props.value}"
        },
        "transforms": [
          {
            "code": "\tHasLimits \u003d False\n\t\n\tif value \u003e 0:\n\t\tfor (k, v) in system.dataset.toPyDataSet(self.getChild(\"root\").getChild(\"Metadata\").getChild(\"MetadataDatatype\").getChild(\"Datatype\").props.options):\n\t\t\tif value \u003d\u003d k and (v \u003d\u003d \u0027Integer\u0027 or v \u003d\u003d \u0027Float\u0027):\n\t\t\t\tHasLimits \u003d True\n\t\n\treturn HasLimits",
            "type": "script"
          }
        ],
        "type": "expr"
      },
      "onChange": {
        "enabled": null,
        "script": "\tif currentValue.value \u003d\u003d False:\n\t\tself.getChild(\"root\").getChild(\"Limits\").getChild(\"LimitsHiHi\").getChild(\"HiHiValue\").props.value \u003d None\n\t\tself.getChild(\"root\").getChild(\"Limits\").getChild(\"LimitsHi\").getChild(\"HiValue\").props.value \u003d None\n\t\tself.getChild(\"root\").getChild(\"Limits\").getChild(\"LimitsTarget\").getChild(\"Target\").props.value \u003d None\n\t\tself.getChild(\"root\").getChild(\"Limits\").getChild(\"LimitsLo\").getChild(\"LoValue\").props.value \u003d None\n\t\tself.getChild(\"root\").getChild(\"Limits\").getChild(\"LimitsLoLo\").getChild(\"LoLoValue\").props.value \u003d None"
      },
      "persistent": true
    },
    "custom.Hi": {
