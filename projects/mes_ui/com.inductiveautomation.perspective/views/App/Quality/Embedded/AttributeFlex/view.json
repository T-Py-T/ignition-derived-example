{
  "custom": {
    "AlertColors": {
      "Hi": "yellow",
      "HiHi": "red",
      "Lo": "yellow",
      "LoLo": "red",
      "Target": "value"
    },
    "AttributeID": 131,
    "AttributeName": "Attribute Name",
    "CurrentValue": "",
    "Datatype": "Float"
  },
  "params": {
    "AttributeID": 131,
    "AttributeName": "Attribute Name",
    "Datatype": "Float",
    "Hi": 50,
    "HiHi": 100,
    "InsertValue": "value",
    "Lo": 10,
    "LoLo": 5,
    "SavedValue": "",
    "Target": "value"
  },
  "propConfig": {
    "custom.AlertColors": {
      "persistent": true
    },
    "custom.AttributeID": {
      "binding": {
        "config": {
          "path": "view.params.AttributeID"
        },
        "type": "property"
      },
      "persistent": true
    },
    "custom.AttributeName": {
      "binding": {
        "config": {
          "path": "view.params.AttributeName"
        },
        "type": "property"
      },
      "persistent": true
    },
    "custom.CurrentValue": {
      "persistent": true
    },
    "custom.Datatype": {
      "binding": {
        "config": {
          "path": "view.params.Datatype"
        },
        "type": "property"
      },
      "persistent": true
    },
    "params.AttributeID": {
      "paramDirection": "inout",
      "persistent": true
    },
    "params.AttributeName": {
      "paramDirection": "inout",
      "persistent": true
    },
    "params.Datatype": {
      "paramDirection": "inout",
      "persistent": true
    },
    "params.Hi": {
      "paramDirection": "inout",
      "persistent": true
    },
    "params.HiHi": {
      "paramDirection": "inout",
      "persistent": true
    },
    "params.InsertValue": {
      "paramDirection": "inout",
      "persistent": true
    },
    "params.Lo": {
      "paramDirection": "inout",
      "persistent": true
    },
    "params.LoLo": {
      "paramDirection": "inout",
      "persistent": true
    },
    "params.SavedValue": {
      "paramDirection": "inout",
      "persistent": true
    },
    "params.Target": {
      "paramDirection": "inout",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 40,
      "width": 308
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "AttributeLabel1"
        },
        "position": {
          "basis": "40%"
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.AttributeName"
              },
              "transforms": [
                {
                  "expression": "{value}+\u0027: \u0027",
                  "type": "expression"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "Global/MarginBottom Global/MarginLeft TextFields/Root TextFields/TextTitle"
          },
          "textStyle": {
            "classes": "TextFields/TextTitle",
            "textAlign": "left"
          }
        },
        "type": "ia.display.label"
      },
      {
        "events": {
          "dom": {
            "onBlur": {
              "config": {
                "script": "\tself.removeTooltip()"
              },
              "scope": "G",
              "type": "script"
            },
            "onFocus": {
              "config": {
                "script": "\tself.requestTooltip()"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "AttributeValue1",
          "tooltip": {
            "enabled": true,
            "location": "center",
            "style": {
              "visibility": "visible"
            }
          }
        },
        "position": {
          "basis": "32px",
          "grow": 1
        },
        "propConfig": {
          "meta.tooltip.text": {
            "binding": {
              "config": {
                "expression": "\u0027HiHi: \u0027 + {view.params.HiHi} +\r\n\u0027 | Hi: \u0027 + {view.params.Hi} +\r\n\u0027 | Lo: \u0027 + {view.params.Lo} +\r\n\u0027 | LoLo: \u0027 + {view.params.LoLo}"
              },
              "type": "expr"
            }
          },
          "position.display": {
            "binding": {
              "config": {
                "expression": "if({view.custom.Datatype} \u003d \u0027Integer\u0027 ||{view.custom.Datatype} \u003d \u0027Float\u0027, true, false)"
              },
              "type": "expr"
            }
          },
          "props.style.borderColor": {
            "binding": {
              "config": {
                "expression": "if({view.params.SavedValue} \u003e {view.params.HiHi}, {view.custom.AlertColors.HiHi},\r\nif({view.params.SavedValue} \u003e {view.params.Hi}, {view.custom.AlertColors.Hi},\r\nif({view.params.SavedValue} \u003c {view.params.LoLo}, {view.custom.AlertColors.LoLo},\r\nif({view.params.SavedValue} \u003c {view.params.Lo}, {view.custom.AlertColors.Lo},\r\n \u0027\u0027))))"
              },
              "overlayOptOut": true,
              "type": "expr"
            }
          },
          "props.value": {
            "binding": {
              "config": {
                "expression": "{view.params.SavedValue}"
              },
              "type": "expr"
            },
            "onChange": {
              "enabled": null,
              "script": "\tif previousValue !\u003d currentValue:\n\t\tself.view.params.SavedValue \u003d currentValue"
            }
          }
        },
        "props": {
          "align": "left",
          "format": "0,0.00",
          "placeholder": "Please Enter a Number....",
          "style": {
            "borderStyle": "solid",
            "classes": "Global/MarginLeft TextFields/TextField",
            "overflow": "hidden"
          }
        },
        "type": "ia.input.numeric-entry-field"
      },
      {
        "meta": {
          "name": "AttributeValue6",
          "tooltip": {
            "location": "top"
          }
        },
        "position": {
          "basis": "32px",
          "grow": 1
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "expression": "if({view.custom.Datatype} \u003d \u0027Boolean\u0027, true, false)"
              },
              "type": "expr"
            }
          },
          "props.selected": {
            "binding": {
              "config": {
                "expression": "{view.params.SavedValue}"
              },
              "type": "expr"
            },
            "onChange": {
              "enabled": null,
              "script": "\tif previousValue !\u003d currentValue:\n\t\tself.view.params.SavedValue \u003d currentValue"
            }
          },
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.AttributeName"
              },
              "transforms": [
                {
                  "fallback": "",
                  "inputType": "scalar",
                  "mappings": [],
                  "outputType": "scalar",
                  "type": "map"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "TextFields/TextField"
          }
        },
        "type": "ia.input.checkbox"
      },
      {
        "meta": {
          "name": "TextField",
          "tooltip": {
            "location": "top",
            "style": {
              "visibility": "visible"
            },
            "text": "Testing123"
          }
        },
        "position": {
          "basis": "70%",
          "grow": 1
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "expression": "if({view.custom.Datatype} \u003d \u0027String\u0027 ||{view.custom.Datatype} \u003d \u0027Varchar\u0027, true, false)"
              },
              "type": "expr"
            }
          },
          "props.text": {
            "binding": {
              "config": {
                "expression": "{view.params.SavedValue}"
              },
              "type": "expr"
            },
            "onChange": {
              "enabled": null,
              "script": "\tif previousValue !\u003d currentValue:\n\t\tself.view.params.SavedValue \u003d currentValue"
            }
          }
        },
        "props": {
          "placeholder": "Enter Text Here....",
          "style": {
            "classes": "Global/MarginLeft TextFields/TextField",
            "overflow": "hidden"
          }
        },
        "type": "ia.input.text-field"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "style": {
        "classes": "Global/MarginBottom",
        "overflow": "hidden"
      }
    },
    "type": "ia.container.flex"
  }
}