                    "onClick": [
                      {
                        "config": {
                          "script": "\t# Grabs values from inputs\n#\tdate \u003d self.parent.parent.parent.getChild(\"DateSelection/Input/Content/StartDate\").props.value\n\tshift \u003d self.parent.parent.parent.getChild(\"Shift/Input/Content/Dropdown\").props.value\n#\tscrapCategory \u003d self.parent.parent.parent.getChild(\"ScrapCategory/Input/Content/TextField\").props.text\n#\tscrapReason \u003d self.parent.parent.parent.getChild(\"ScrapReason/Input/Content/TextField\").props.text\n#\tscrapQuantity \u003d self.parent.parent.parent.getChild(\"Quantity/Input/Content/TextField\").props.text\n\n\tif shift \u003d\u003d \"\":\n\t\tself.parent.parent.parent.getChild(\"Shift/Input/Content/Dropdown\").props.style.classes \u003d gpa.frontEnd.localUI.Constants.DROP_DOWN_ERROR\n\t\tself.parent.parent.parent.getChild(\"Shift/Input/Content/ValidationIcon\").position.display \u003d True\n\t\tself.parent.parent.parent.getChild(\"Shift/Input/ErrorValidation\").position.display \u003d True\n\t\n\t\n#\tinventory \u003d self.parent.parent.parent.getChild(\"Inventory/Input/Content/InventorySelection\").props.value\n#\tlabel \u003d self.parent.parent.parent.getChild(\"Label/Input/Content/TextField\").props.text\n#\tproduct \u003d self.parent.parent.parent.getChild(\"ProductCode/Input/Content/TextField\").props.text\n#\t\n#\tif inventory \u003d\u003d \"\":\n#\t\tself.parent.parent.parent.getChild(\"Inventory/Input/Content/InventorySelection\").props.style.classes \u003d localUI.Constants.DROP_DOWN_ERROR\n#\t\tself.parent.parent.parent.getChild(\"Inventory/Input/Content/ValidationIcon\").position.display \u003d True\n#\t\tself.parent.parent.parent.getChild(\"Inventory/Input/ErrorValidation\").position.display \u003d True\n#\t\n#\tif label \u003d\u003d \"\":\n#\t\tself.parent.parent.parent.getChild(\"Label/Input/Content/TextField\").props.style.classes \u003d localUI.Constants.TEXT_ERROR\n#\t\tself.parent.parent.parent.getChild(\"Label/Input/Content/ValidationIcon\").position.display \u003d True\n#\t\tself.parent.parent.parent.getChild(\"Label\").getChild(\"Input\").getChild(\"ErrorValidation\").position.display \u003d True\n#\t\t\n#\tif product \u003d\u003d \"\":\n#\t\tself.parent.parent.parent.getChild(\"ProductCode/Input/Content/TextField\").props.style.classes \u003d localUI.Constants.TEXT_ERROR\n#\t\tself.parent.parent.parent.getChild(\"ProductCode/Input/Content/ValidationIcon\").position.display \u003d True\n#\t\tself.parent.parent.parent.getChild(\"ProductCode/Input/ErrorValidation\").position.display \u003d True\n#\t\t\n#\tif inventory !\u003d \"\" and label !\u003d \"\" and product !\u003d \"\":\n#\t\tsystem.perspective.print(\"sending data\")"
                        },
                        "enabled": false,
                        "scope": "G",
                        "type": "script"
                      },
                      {
                        "config": {
                          "script": "\tmaterialID \u003d self.view.custom.MaterialID\n\tmaterialLotID \u003d self.view.custom.MaterialLotID\n\tlotNumber \u003d self.view.custom.LotNumberUpdate\n\tquantity \u003d self.view.custom.Quantity\n\tqueryPath \u003d \"app/Schedule/editMaterialLot\"\n\t\n\tparams \u003d {\n\t\t\"materialLotID\": materialLotID,\n\t\t\"lotNumber\": lotNumber,\n\t\t\"quantity\": quantity,\n\t\t\"materialID\": materialID\n\t}\n\t\n\tsystem.db.runNamedQuery(queryPath, params)\n\tsystem.perspective.sendMessage(\"addedMaterial\") #Refresh materials\n\tsystem.perspective.sendMessage(\"addedMaterialLot\") #Refresh Material Lots\n\tsystem.perspective.closePopup(self.view.params.ModalName)\t\n"
                        },
                        "scope": "G",
