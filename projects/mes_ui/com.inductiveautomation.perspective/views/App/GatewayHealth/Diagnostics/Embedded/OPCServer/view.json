    "Server": "Ignition OPC UA Server"
  },
  "propConfig": {
    "custom.Connected": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "1": "{view.params.EqPath}",
            "2": "{view.params.Server}"
          },
          "tagPath": "{1}/OPC/Connections/{2}/Connected"
        },
        "transforms": [
          {
            "fallback": "No",
            "inputType": "scalar",
            "mappings": [
              {
                "input": false,
                "output": "No"
              },
              {
                "input": true,
                "output": "Yes"
              }
            ],
            "outputType": "scalar",
            "type": "map"
          }
        ],
        "type": "tag"
      },
      "persistent": true
    },
    "custom.Enabled": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
