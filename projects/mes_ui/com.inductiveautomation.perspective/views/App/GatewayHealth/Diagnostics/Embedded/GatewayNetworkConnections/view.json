{
  "custom": {
    "Instances": []
  },
  "params": {
    "EqPath": "[App1_System]Gateway",
    "State": ""
  },
  "propConfig": {
    "custom.Instances": {
      "binding": {
        "config": {
          "path": "view.params.EqPath"
        },
        "overlayOptOut": true,
        "transforms": [
          {
            "code": "\tinstances \u003d []\n\tstartPath \u003d value + \"/Gateway Network\"\n\tres \u003d system.tag.browse(path\u003dstartPath)\n\tsystem.perspective.print(res)\n\tfor result in res.getResults():\n\t\tif result.get(\u0027hasChildren\u0027):\n\t\t\tnetworkPath \u003d result.get(\u0027fullPath\u0027)\n\t\t\ttagValues \u003d system.tag.browse(str(networkPath))\n\t\t\tstate \u003d \u0027\u0027\n\t\t\tfor tagValue in tagValues:\n\t\t\t\tif tagValue.get(\u0027name\u0027) \u003d\u003d \u0027IsAvailable\u0027:\n\t\t\t\t\tstate \u003d tagValue.get(\u0027value\u0027)\n\t\t\t\t\t\t\n\t\t\tinstances.append({\"Server\":result[\"name\"], \"EqPath\": self.view.params.EqPath, \"State\": state})\n\n\treturn instances",
            "type": "script"
          }
        ],
        "type": "property"
      },
      "persistent": true
    },
    "params.EqPath": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.State": {
      "binding": {
        "config": {
          "path": "/root/FlexRepeater.props.instances"
        },
        "enabled": false,
        "overlayOptOut": true,
        "transforms": [
          {
            "code": "\tfor instance in value:\n\t\tif instance[\"State\"] \u003d\u003d False:\n\t\t\treturn gpa.frontEnd.localUI.Constants.ERROR\n\t\telse:\n\t\t\treturn \"\"",
            "type": "script"
          }
        ],
        "type": "property"
      },
      "paramDirection": "output",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "width": 265
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "FlexRepeater"
        },
        "propConfig": {
          "props.instances": {
            "binding": {
              "config": {
                "path": "view.custom.Instances"
              },
              "overlayOptOut": true,
              "type": "property"
            }
          }
        },
        "props": {
          "direction": "column",
          "elementPosition": {
            "basis": "auto",
            "grow": 0,
            "shrink": 0
          },
          "path": "App/GatewayHealth/Diagnostics/Embedded/GatewayNetworkConnection",
          "useDefaultViewHeight": false,
          "useDefaultViewWidth": false
        },
        "type": "ia.display.flex-repeater"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column"
    },
    "type": "ia.container.flex"
  }
}