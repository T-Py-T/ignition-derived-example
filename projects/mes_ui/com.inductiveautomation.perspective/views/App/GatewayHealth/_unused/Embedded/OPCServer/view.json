{
  "custom": {},
  "params": {
    "Server": "Ignition OPC UA Server",
    "TagPath": "value"
  },
  "propConfig": {
    "params.Server": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.TagPath": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 200,
      "width": 300
    }
  },
  "root": {
    "children": [
      {
        "children": [
          {
            "meta": {
              "name": "Value"
            },
            "position": {
              "shrink": 0
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "path": "view.params.Server"
                  },
                  "type": "property"
                }
              }
            },
            "props": {
              "style": {
                "classes": "Cards/Common/CardTitle"
              }
            },
            "type": "ia.display.label"
          }
        ],
        "meta": {
          "name": "Name"
        },
        "position": {
          "basis": "20px",
          "shrink": 0
        },
        "props": {
          "style": {
            "classes": "Cards/Common/CardTitleBorder",
            "overflow": "hidden"
          },
          "wrap": "wrap"
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "meta": {
              "name": "Label"
            },
            "position": {
              "grow": 1
            },
            "props": {
              "style": {
                "classes": "Cards/Common/CardText"
              },
              "text": "Is Enabled"
            },
            "type": "ia.display.label"
          },
          {
            "meta": {
              "name": "Value"
            },
            "position": {
              "basis": "30px",
              "shrink": 0
            },
            "propConfig": {
              "props.style.backgroundColor": {
                "binding": {
                  "config": {
                    "fallbackDelay": 2.5,
                    "mode": "indirect",
                    "references": {
                      "server": "{view.params.Server}",
                      "tagPath": "{view.params.TagPath}"
                    },
                    "tagPath": "{tagPath}/{server}/Enabled"
                  },
                  "transforms": [
                    {
                      "fallback": "var(--newGrey)",
                      "inputType": "scalar",
                      "mappings": [
                        {
                          "input": false,
                          "output": "var(--error)"
                        },
                        {
                          "input": true,
                          "output": "var(--success)"
                        }
                      ],
                      "outputType": "scalar",
                      "type": "map"
                    }
                  ],
                  "type": "tag"
                }
              },
              "props.text": {
                "binding": {
                  "config": {
                    "fallbackDelay": 2.5,
                    "mode": "indirect",
                    "references": {
                      "server": "{view.params.Server}",
                      "tagPath": "{view.params.TagPath}"
                    },
                    "tagPath": "{tagPath}/{server}/Enabled"
                  },
                  "transforms": [
                    {
                      "fallback": "No",
                      "inputType": "scalar",
                      "mappings": [
                        {
                          "input": true,
                          "output": "Yes"
                        },
                        {
                          "input": false,
                          "output": "No"
                        }
                      ],
                      "outputType": "scalar",
                      "type": "map"
                    }
                  ],
                  "type": "tag"
                }
              }
            },
            "props": {
              "style": {
                "classes": "Cards/Common/Boolean"
              }
            },
            "type": "ia.display.label"
          }
        ],
        "meta": {
          "name": "Is Enabled"
        },
        "position": {
          "basis": "20px",
          "shrink": 0
        },
        "props": {
          "style": {
            "marginBottom": "2px",
            "overflow": "hidden"
          },
          "wrap": "wrap"
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
