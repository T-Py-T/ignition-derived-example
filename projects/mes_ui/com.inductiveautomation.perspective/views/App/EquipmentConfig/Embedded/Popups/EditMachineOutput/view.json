        "transforms": [
          {
            "code": "\ttagPath \u003d value + \u0027/machineOutput/\u0027 + \u0027Parameters\u0027\n\ttags \u003d system.tag.browse(tagPath)\n\t\n\ttagDict \u003d {}\n\t\n\tfor tag in tags.getResults():\n\t\ttagFullPath \u003d str(tag[\u0027fullPath\u0027])\n\t\ttagValue \u003d system.tag.read(tagFullPath).value\n\t\ttagDict[tag[\u0027name\u0027]] \u003d tagValue\n\t\t\n\treturn tagDict",
            "type": "script"
          }
        ],
        "type": "property"
      },
      "persistent": true
    },
    "custom.FinalParams": {
      "binding": {
        "config": {
          "path": "view.params.EqData"
        },
        "transforms": [
          {
            "code": "\tself.custom.FinalParams \u003d value\n\tself.custom.FinalParams[\u0027Parameters\u0027] \u003d self.custom.SelectedParameters\n\t\n\treturn self.custom.FinalParams",
            "type": "script"
          }
        ],
        "type": "property"
      },
      "persistent": true
    },
