{
  "custom": {},
  "params": {
    "isAll": false,
    "locale": "en",
    "name": "english"
  },
  "propConfig": {
    "params.isAll": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.locale": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.name": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 25,
      "width": 150
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Checkbox"
        },
        "position": {
          "basis": "36px",
          "grow": 1
        },
        "propConfig": {
          "props.selected": {
            "onChange": {
              "enabled": null,
              "script": "\tif origin \u003d\u003d \"Browser\":\n\t\t#adapt scope\n\t\tif self.view.params.isAll:\n\t\t\tscope \u003d \"page\"\n\t\telse:\n\t\t\tscope\u003d\u0027view\u0027\n\t\t\n\t\t#Check if selected or unselected\t\n\t\tif currentValue.value:\n\t\t\tmessageType \u003d \"isSelected\"\n\t\t\tpayload\u003d {\u0027locale\u0027:self.view.params.locale,\n\t\t\t\t\t  \u0027isAll\u0027:self.view.params.isAll}\n\t\t\tsystem.perspective.sendMessage(messageType, payload, scope\u003dscope)\n\t\telse:\n\t\t\tmessageType \u003d \"isUnselected\"\n\t\t\tpayload\u003d {\u0027locale\u0027:self.view.params.locale,\n\t\t\t\t\t  \u0027isAll\u0027:self.view.params.isAll}\n\t\t\tsystem.perspective.sendMessage(messageType, payload, scope\u003dscope)"
            }
          },
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.name"
              },
              "type": "property"
            }
          }
        },
        "type": "ia.input.checkbox"
      },
      {
        "events": {
          "dom": {
            "onClick": {
              "config": {
                "draggable": true,
                "id": "DelLanguage",
                "modal": false,
                "overlayDismiss": false,
                "resizable": false,
                "showCloseIcon": true,
                "title": "Confrim Delete",
                "type": "open",
                "viewParams": {
                  "language": "{view.params.name}",
                  "locale": "{view.params.locale}"
                },
                "viewPath": "TranslationsManager/Popups/DelLanguage",
                "viewportBound": false
              },
              "scope": "C",
              "type": "popup"
            }
          }
        },
        "meta": {
          "name": "Delete",
          "tooltip": {
            "enabled": true,
            "text": "Remove Language"
          }
        },
        "position": {
          "basis": "30px"
        },
        "propConfig": {
          "meta.visible": {
            "binding": {
              "config": {
                "struct": {
                  "isAll": "{view.params.isAll}",
                  "local": "{view.params.locale}"
                },
                "waitOnAll": true
              },
              "transforms": [
                {
                  "expression": "!{value}[\u0027isAll\u0027] \u0026\u0026 !({value}[\u0027local\u0027] \u003d \"en\")",
                  "type": "expression"
                }
              ],
              "type": "expr-struct"
            }
          }
        },
        "props": {
          "path": "material/delete_outline"
        },
        "type": "ia.display.icon"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "style": {
        "borderBottomColor": "#D5D5D5",
        "borderBottomStyle": "solid",
        "borderBottomWidth": "1px",
        "overflow": "hidden"
      }
    },
    "scripts": {
      "customMethods": [],
      "extensionFunctions": null,
      "messageHandlers": [
        {
          "messageType": "isSelected",
          "pageScope": true,
          "script": "\tisAll \u003d payload[\u0027isAll\u0027]\n\t\n\tif isAll:\n\t\tself.getChild(\"Checkbox\").props.selected \u003d True\n\t\n\t#Do nothing if event is comming from All checkbox\n\tif not self.view.params.isAll:\n\t\tmessageType \u003d \"updateSelectedLocales\"\n\t\tpayload\u003d {\u0027locale\u0027:self.view.params.locale,\n\t\t\t\t  \u0027action\u0027:True}\n\t\tsystem.perspective.sendMessage(messageType, payload)\n\t",
          "sessionScope": false,
          "viewScope": true
        },
        {
          "messageType": "isUnselected",
          "pageScope": true,
          "script": "\tisAll \u003d payload[\u0027isAll\u0027]\n\t\n\tif isAll:\n\t\tself.getChild(\"Checkbox\").props.selected \u003d False\n\telse:\t\n\t\t#send a message to unselect all checkbock is one checkbock is unselected\n\t\tmessageType \u003d \"unselectAllCheck\"\n\t\tsystem.perspective.sendMessage(messageType, payload\u003d[])\n\t\n\t#Do nothing if event is comming from All checkbox\n\tif not self.view.params.isAll:\n\t\tmessageType \u003d \"updateSelectedLocales\"\n\t\tpayload\u003d {\u0027locale\u0027:self.view.params.locale,\n\t\t\t\t  \u0027action\u0027:False}\n\t\tsystem.perspective.sendMessage(messageType, payload)",
          "sessionScope": false,
          "viewScope": true
        },
        {
          "messageType": "unselectAllCheck",
          "pageScope": true,
          "script": "\tisAllParam \u003d self.view.params.isAll\n\t\n\tif isAllParam:\n\t\tself.getChild(\"Checkbox\").props.selected \u003d False\n",
          "sessionScope": false,
          "viewScope": false
        }
      ]
    },
    "type": "ia.container.flex"
  }
}