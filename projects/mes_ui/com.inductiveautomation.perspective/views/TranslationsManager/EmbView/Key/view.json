{
  "custom": {},
  "params": {
    "value": ""
  },
  "propConfig": {
    "params.value": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 25,
      "width": 150
    }
  },
  "root": {
    "children": [
      {
        "events": {
          "dom": {
            "onClick": {
              "config": {
                "draggable": true,
                "id": "DelKey",
                "modal": false,
                "overlayDismiss": false,
                "resizable": false,
                "showCloseIcon": true,
                "title": "Confirm Delete",
                "type": "open",
                "viewParams": {
                  "key": "{view.params.value}"
                },
                "viewPath": "TranslationsManager/Popups/DelKey",
                "viewportBound": false
              },
              "scope": "C",
              "type": "popup"
            }
          }
        },
        "meta": {
          "name": "Delete",
          "tooltip": {
            "enabled": true,
            "text": "Remove Term"
          }
        },
        "position": {
          "basis": "30px"
        },
        "props": {
          "path": "material/delete_outline"
        },
        "type": "ia.display.icon"
      },
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "basis": "50px",
          "grow": 1
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.value"
              },
              "type": "property"
            }
          }
        },
        "type": "ia.display.label"
      }
    ],
    "meta": {
      "name": "root"
    },
    "type": "ia.container.flex"
  }
}