{
  "custom": {},
  "params": {
    "key": "key"
  },
  "propConfig": {
    "params.key": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 100,
      "width": 399
    }
  },
  "root": {
    "children": [
      {
        "children": [
          {
            "meta": {
              "name": "Icon"
            },
            "position": {
              "basis": "30px"
            },
            "props": {
              "path": "material/warning",
              "style": {
                "marginRight": "5px"
              }
            },
            "type": "ia.display.icon"
          },
          {
            "meta": {
              "name": "Label"
            },
            "position": {
              "basis": "32px",
              "grow": 1
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "path": "view.params.key"
                  },
                  "transforms": [
                    {
                      "code": "\tquestion \u003d \"Are you sure you want to remove the term \u0027%s\u0027? This will also delete all translations\" %value\n\treturn question",
                      "type": "script"
                    }
                  ],
                  "type": "property"
                }
              }
            },
            "type": "ia.display.label"
          }
        ],
        "meta": {
          "name": "Question"
        },
        "position": {
          "basis": "200px"
        },
        "props": {
          "alignItems": "center",
          "style": {
            "marginBottom": "5px"
          }
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "events": {
              "dom": {
                "onClick": {
                  "config": {
                    "script": "\tmessageType \u003d \"delKey\"\n\tpayload \u003d {\u0027key\u0027:self.view.params.key}\n\tsystem.perspective.sendMessage(messageType, payload,scope\u003d\"page\")"
                  },
                  "scope": "G",
                  "type": "script"
                }
              }
            },
            "meta": {
              "name": "Yes"
            },
            "position": {
              "basis": "80px"
            },
            "props": {
              "text": "Yes"
            },
            "type": "ia.input.button"
          },
          {
            "events": {
              "dom": {
                "onClick": {
                  "config": {
                    "script": "\t#Close popup\n\tid \u003d \u0027DelKey\u0027\n\tsystem.perspective.closePopup(id)"
                  },
                  "scope": "G",
                  "type": "script"
                }
              }
            },
            "meta": {
              "name": "No"
            },
            "position": {
              "basis": "80px"
            },
            "props": {
              "primary": false,
              "text": "No"
            },
            "type": "ia.input.button"
          }
        ],
        "meta": {
          "name": "Control"
        },
        "position": {
          "basis": "200px"
        },
        "props": {
          "justify": "space-around"
        },
        "type": "ia.container.flex"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "margin": "5px",
        "overflow": "hidden"
      }
    },
    "type": "ia.container.flex"
  }
}