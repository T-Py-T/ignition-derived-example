{
  "custom": {},
  "params": {
    "Message": "ttesting",
    "ModalName": "value",
    "Title": "Test",
    "ViewPath": "GlobalComponents/Modals/Embedded/Message"
  },
  "propConfig": {
    "params.Message": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.ModalName": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.Title": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.ViewPath": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 216,
      "width": 402
    }
  },
  "root": {
    "children": [
      {
        "children": [
          {
            "meta": {
              "name": "ModalTitle"
            },
            "position": {
              "basis": "50px",
              "grow": 1
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "path": "view.params.Title"
                  },
                  "type": "property"
                }
              }
            },
            "props": {
              "style": {
                "classes": "Global/MarginAllLeft"
              },
              "textStyle": {
                "color": "white"
              }
            },
            "type": "ia.display.label"
          },
          {
            "events": {
              "dom": {
                "onClick": {
                  "config": {
                    "script": "\tmodalName \u003d self.view.params.ModalName\n\tsystem.perspective.closePopup(modalName)"
                  },
                  "scope": "G",
                  "type": "script"
                }
              }
            },
            "meta": {
              "name": "CloseIcon"
            },
            "position": {
              "basis": "25px"
            },
            "props": {
              "color": "white",
              "path": "material/close",
              "style": {
                "classes": "Global/MarginAllRight"
              }
            },
            "type": "ia.display.icon"
          }
        ],
        "meta": {
          "name": "Row1"
        },
        "position": {
          "basis": "31px"
        },
        "props": {
          "style": {
            "backgroundColor": "var(--newGrey)",
            "classes": "\n"
          }
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "meta": {
              "name": "View"
            },
            "position": {
              "basis": "320px",
              "grow": 1
            },
            "propConfig": {
              "props.params.Message": {
                "binding": {
                  "config": {
                    "path": "view.params.Message"
                  },
                  "type": "property"
                }
              },
              "props.path": {
                "binding": {
                  "config": {
                    "path": "view.params.ViewPath"
                  },
                  "type": "property"
                }
              }
            },
            "type": "ia.display.view"
          }
        ],
        "meta": {
          "name": "Row2"
        },
        "position": {
          "basis": "200px"
        },
        "type": "ia.container.flex"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column"
    },
    "type": "ia.container.flex"
  }
}