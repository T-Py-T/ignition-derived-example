        },
        "type": "expr"
      },
      "persistent": true
    },
    "custom.Success": {
      "binding": {
        "config": {
          "expression": "if(indexOf({/root/FlexContainer/Markdown.props.source}, \"success\") \u003e\u003d 0||indexOf({/root/FlexContainer/Markdown.props.source}, \"Success\") \u003e\u003d 0, true, false)"
        },
