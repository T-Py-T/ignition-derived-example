{
  "custom": {},
  "params": {},
  "props": {
    "defaultSize": {
      "height": 65
    }
  },
  "root": {
    "children": [
      {
        "events": {
          "dom": {
            "onClick": {
              "config": {
                "id": "navBar",
                "type": "toggle"
              },
              "scope": "C",
              "type": "dock"
            }
          }
        },
        "meta": {
          "name": "Icon"
        },
        "position": {
          "basis": "30px",
          "shrink": 0
        },
        "props": {
          "color": "--div-11",
          "path": "material/menu",
          "style": {
            "classes": "MES/DockedViews/Header/Icons"
          }
        },
        "type": "ia.display.icon"
      },
      {
        "meta": {
          "name": "spacing"
        },
        "position": {
          "basis": "50px",
          "grow": 1
        },
        "type": "ia.display.label"
      },
      {
        "children": [
          {
            "events": {
              "dom": {
                "onClick": {
                  "config": {
                    "draggable": false,
                    "id": "themes",
                    "modal": false,
                    "overlayDismiss": true,
                    "position": {
                      "height": 272,
                      "right": 10,
                      "top": 50,
                      "width": 175
                    },
                    "positionType": "exact",
                    "resizable": false,
                    "showCloseIcon": false,
                    "type": "toggle",
                    "viewPath": "Docked Views/Header/Popups/Themes",
                    "viewportBound": false
                  },
                  "scope": "C",
                  "type": "popup"
                }
              }
            },
            "meta": {
              "name": "Themes"
            },
            "position": {
              "basis": "30px",
              "shrink": 0
            },
            "propConfig": {
              "props.path": {
                "binding": {
                  "config": {
                    "path": "session.props.theme"
                  },
                  "transforms": [
                    {
                      "fallback": "material/wb_sunny",
                      "inputType": "scalar",
                      "mappings": [
                        {
                          "input": "light",
                          "output": "material/wb_sunny"
                        },
                        {
                          "input": "light-cool",
                          "output": "material/wb_sunny"
                        },
                        {
                          "input": "light-warm",
                          "output": "material/wb_sunny"
                        },
                        {
                          "input": "dark",
                          "output": "material/nights_stay"
                        },
                        {
                          "input": "dark-cool",
                          "output": "material/nights_stay"
                        },
                        {
                          "input": "dark-warm",
                          "output": "material/nights_stay"
                        }
                      ],
                      "outputType": "scalar",
                      "type": "map"
                    }
                  ],
                  "type": "property"
                }
              }
            },
            "props": {
              "color": "--div-11",
              "style": {
                "marginRight": "10px"
              }
            },
            "type": "ia.display.icon"
          },
          {
            "meta": {
              "name": "Sub Theme"
            },
            "position": {
              "basis": "20px",
              "shrink": 0
            },
            "propConfig": {
              "props.path": {
                "binding": {
                  "config": {
                    "path": "session.props.theme"
                  },
                  "transforms": [
                    {
                      "fallback": "",
                      "inputType": "scalar",
                      "mappings": [
                        {
                          "input": "light-cool",
                          "output": "material/ac_unit"
                        },
                        {
                          "input": "dark-cool",
                          "output": "material/ac_unit"
                        },
                        {
                          "input": "light-warm",
                          "output": "material/whatshot"
                        },
                        {
                          "input": "dark-warm",
                          "output": "material/whatshot"
                        }
                      ],
                      "outputType": "scalar",
                      "type": "map"
                    }
                  ],
                  "type": "property"
                }
              }
            },
            "props": {
              "color": "--div-11",
              "style": {
                "height": "20px",
                "position": "absolute",
                "transform": "translate(18px, 5px)",
                "width": "20px"
              }
            },
            "type": "ia.display.icon"
          }
        ],
        "meta": {
          "name": "Themes"
        },
        "position": {
          "basis": "30px",
          "shrink": 0
        },
        "props": {
          "style": {
            "classes": "MES/DockedViews/Header/Icons",
            "overflow": "visible"
          }
        },
        "type": "ia.container.flex"
      },
      {
        "events": {
          "dom": {
            "onClick": {
              "config": {
                "id": "alarmBar",
                "type": "toggle"
              },
              "scope": "C",
              "type": "dock"
            }
          }
        },
        "meta": {
          "name": "Alarms"
        },
        "position": {
          "basis": "30px",
          "shrink": 0
        },
        "propConfig": {
          "props.path": {
            "binding": {
              "config": {
                "fallbackDelay": 2.5,
                "mode": "direct",
                "tagPath": "[default]PreparedFoodsLine/now1"
              },
              "transforms": [
                {
                  "code": "\talarms \u003d system.alarm.queryStatus(state \u003d \"ActiveUnacked\").getDataset()\n\treturn alarms.getRowCount()",
                  "type": "script"
                },
                {
                  "fallback": null,
                  "inputType": "range",
                  "mappings": [
                    {
                      "input": {
                        "min": 0
                      },
                      "output": "material/notifications_active"
                    },
                    {
                      "input": {
                        "max": 0
                      },
                      "output": "material/notifications"
                    }
                  ],
                  "outputType": "scalar",
                  "type": "map"
                }
              ],
              "type": "tag"
            }
          },
          "props.style.classes": {
            "binding": {
              "config": {
                "path": "this.props.path"
              },
              "enabled": false,
              "transforms": [
                {
                  "fallback": "DockedViews/Header/Icons, DockedViews/Header/Alarms_Active",
                  "inputType": "scalar",
                  "mappings": [
                    {
                      "input": "material/notifications",
                      "output": "DockedViews/Header/Icons"
                    }
                  ],
                  "outputType": "scalar",
                  "type": "map"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "color": "--div-11",
          "style": {
            "classes": "MES/DockedViews/Header/Icons"
          }
        },
        "type": "ia.display.icon"
      },
      {
        "meta": {
          "name": "Login"
        },
        "position": {
          "basis": "30px",
          "shrink": 0
        },
        "props": {
          "color": "--div-11",
          "path": "material/person",
          "style": {
            "classes": "MES/DockedViews/Header/Icons"
          }
        },
        "type": "ia.display.icon"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "style": {
        "classes": "MES/DockedViews/Header/Body",
        "overflow": "visible"
      }
    },
    "type": "ia.container.flex"
  }
}