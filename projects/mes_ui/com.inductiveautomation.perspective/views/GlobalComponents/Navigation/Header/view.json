{
  "custom": {},
  "params": {
    "Title": "Testing",
    "showEmbedded": false,
    "viewPath": "App/GatewayHealth/Diagnostics/Embedded/SystemSelector"
  },
  "propConfig": {
    "params.Title": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.showEmbedded": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.viewPath": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 50,
      "width": 1479
    }
  },
  "root": {
    "children": [
      {
        "children": [
          {
            "custom": {
              "selectedPath": "value"
            },
            "meta": {
              "name": "Label"
            },
            "position": {
              "grow": 1,
              "shrink": 0
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "path": "view.params.Title"
                  },
                  "type": "property"
                }
              }
            },
            "props": {
              "style": {
                "classes": "Global/MarginAllLeft Text/Heading"
              },
              "textStyle": {
                "color": "--white",
                "textAlign": "left"
              }
            },
            "type": "ia.display.label"
          }
        ],
        "meta": {
          "name": "Title"
        },
        "position": {
          "grow": 1
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "meta": {
              "name": "EmbeddedView"
            },
            "position": {
              "grow": 1
            },
            "propConfig": {
              "position.display": {
                "binding": {
                  "config": {
                    "path": "view.params.showEmbedded"
                  },
                  "transforms": [
                    {
                      "code": "\tif value \u003d\u003d 1:\n\t\treturn True\n\telse:\n\t\treturn False",
                      "type": "script"
                    }
                  ],
                  "type": "property"
                }
              },
              "props.path": {
                "binding": {
                  "config": {
                    "path": "view.params.viewPath"
                  },
                  "type": "property"
                }
              }
            },
            "props": {
              "params": {
                "SelectedPath": "[GatewayHealth]Gateways/App1",
                "selectedGWPath": "[App1_System]Gateway"
              },
              "style": {
                "overflow": "hidden"
              }
            },
            "type": "ia.display.view"
          }
        ],
        "meta": {
          "name": "Title_0"
        },
        "position": {
          "grow": 1
        },
        "type": "ia.container.flex"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "style": {
        "backgroundColor": "--menuBg"
      }
    },
    "type": "ia.container.flex"
  }
}