              "transforms": [
                {
                  "code": "\tpaths \u003d value.split(\u0027/\u0027)\n\ttoString \u003d str(paths[1])\n\tsystem.perspective.print(toString)\n\treturn \u0027\u0027.join([\u0027 \u0027+ s if s.isupper()  else s for s in toString]).lstrip()\n\t",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "params": {
            "navItems": [
              {
                "enabled": true,
                "icon": {
                  "path": "material/home"
                },
                "items": [],
                "label": "Home",
                "style": {
                  "classes": ""
                },
                "target": "/"
              },
              {
                "enabled": true,
                "icon": {
                  "path": "material/pie_chart"
                },
                "items": [],
                "style": {
                  "classes": ""
                },
                "target": ""
              }
            ]
          },
          "path": "StyleGuide/Navigation/PrimaryNav"
        },
        "type": "ia.display.view"
      },
      {
        "meta": {
          "name": "Theme"
        },
        "position": {
          "basis": "200px"
        },
        "props": {
          "path": "StyleGuide/Components/Icons/DarkMode"
        },
        "type": "ia.display.view"
      },
      {
        "meta": {
          "name": "Dropdown"
        },
        "position": {
