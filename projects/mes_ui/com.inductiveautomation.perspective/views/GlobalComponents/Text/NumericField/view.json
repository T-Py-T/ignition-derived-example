{
  "custom": {},
  "params": {
    "DeferUpdates": true,
    "Size": "value",
    "value": ""
  },
  "propConfig": {
    "params.DeferUpdates": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.Size": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.value": {
      "binding": {
        "config": {
          "bidirectional": true,
          "path": "/root/NumericEntryField.props.value"
        },
        "type": "property"
      },
      "paramDirection": "output",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 48,
      "width": 202
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "TextField"
        },
        "position": {
          "display": false,
          "grow": 1
        },
        "propConfig": {
          "position.basis": {
            "binding": {
              "config": {
                "path": "view.params.Size"
              },
              "type": "property"
            }
          },
          "props.deferUpdates": {
            "binding": {
              "config": {
                "path": "view.params.DeferUpdates"
              },
              "type": "property"
            }
          },
          "props.placeholder": {
            "binding": {
              "config": {
                "path": "view.params.Placeholder"
              },
              "type": "property"
            }
          },
          "props.style.classes": {
            "binding": {
              "config": {
                "path": "view.params.Type"
              },
              "transforms": [
                {
                  "fallback": "TextFields/TextField",
                  "inputType": "scalar",
                  "mappings": [
                    {
                      "input": "success",
                      "output": "TextFields/TextSuccess"
                    },
                    {
                      "input": "warning",
                      "output": "TextFields/TextWarning"
                    },
                    {
                      "input": "fail",
                      "output": "TextFields/TextFailure"
                    }
                  ],
                  "outputType": "scalar",
                  "type": "map"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "rejectUpdatesWhileFocused": false,
          "style": {}
        },
        "type": "ia.input.text-field"
      },
      {
        "meta": {
          "name": "NumericEntryField"
        },
        "position": {
          "basis": "150px",
          "grow": 1
        },
        "props": {
          "containerStyle": {
            "marginBottom": 4,
            "marginTop": 4,
            "overflow": "hidden"
          },
          "spinner": {
            "enabled": false
          },
          "style": {
            "classes": "TextFields/TextField",
            "overflow": "hidden"
          }
        },
        "type": "ia.input.numeric-entry-field"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "style": {
        "classes": "TextFields/Root"
      }
    },
    "type": "ia.container.flex"
  }
}