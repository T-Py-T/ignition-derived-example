    "custom.AllLineOEE": {
      "binding": {
        "config": {
          "enableValueCache": false,
          "parameters": {
            "endTime": "now(10000)",
            "siteID": "{session.custom.SelectedSite}",
            "startTime": "tag({view.custom.EQPath}+\u0027/calculatedValues/time/shiftStartTime\u0027)"
          },
          "queryPath": "app/OEE/getReportLinesOEE_AQP"
        },
        "transforms": [
          {
            "code": "    pyData \u003d system.dataset.toPyDataSet(value)\n    NewData \u003d []\n\n    for idx, row in enumerate(pyData):\n        OEE \u003d row[\u0027OEE\u0027] * 100\n        Percent \u003d \u0027{:.2f}%\u0027.format(OEE)\n        A \u003d row[\u0027A\u0027] * 100\n        APercent \u003d \u0027{:.2f}%\u0027.format(A)\n        Q \u003d row[\u0027Q\u0027] * 100\n        QPercent \u003d \u0027{:.2f}%\u0027.format(Q)\n        P \u003d row[\u0027P\u0027] * 100\n        PPercent \u003d \u0027{:.2f}%\u0027.format(P)\n\n        # Determine color based on index\n        if OEE \u003e\u003d 70:\n            color \u003d \u0027#6aaa6d\u0027\n        elif OEE \u003c\u003d 70 and OEE \u003e\u003d 40:\n        \tcolor \u003d \u0027#fd8b2c\u0027\n        else:\n            color \u003d \u0027#f44336\u0027\n\n        data \u003d [row[\u0027Line\u0027], Percent, OEE, APercent, QPercent, PPercent, color]\n        NewData.append(data)\n\n    NewHeaders \u003d [\u0027Name\u0027, \u0027Duration\u0027, \u0027Quantity\u0027, \u0027A\u0027, \u0027Q\u0027, \u0027P\u0027, \u0027Color\u0027]\n    NewDataSet \u003d system.dataset.toDataSet(NewHeaders, NewData)\n    return NewDataSet",
            "type": "script"
          }
        ],
        "type": "query"
      },
      "persistent": true
    },
    "custom.EQPath": {
      "binding": {
        "config": {
          "path": "session.custom.EqPath"
        },
        "type": "property"
      },
      "persistent": true
    },
