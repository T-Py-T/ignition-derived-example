          }
        ],
        "type": "query"
      },
      "persistent": true
    },
    "custom.CountLeft": {
      "binding": {
        "config": {
          "expression": "{view.custom.TargetQuantity}-{view.custom.RunCount}"
        },
        "type": "expr"
