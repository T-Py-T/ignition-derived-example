    "custom.RunCount": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "1": "{view.params.TagPath}",
            "2": "{view.custom.EqPath}"
          },
          "tagPath": "{1}{2}/jobs/currentJob/RunCounts/runGoodParts"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "custom.TargetQuantity": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "1": "{view.params.TagPath}",
            "2": "{view.custom.EqPath}"
          },
          "tagPath": "{1}{2}/jobs/currentJob/scheduleQuantity"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "params.EqPath": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.TagPath": {
      "paramDirection": "input",
      "persistent": true
    }
