                  },
                  "textStyle": {
                    "fontWeight": "bold",
                    "textAlign": "right",
                    "textTransform": "lowercase"
                  }
                },
                "type": "ia.display.label"
              },
              {
                "meta": {
                  "name": "Slash"
                },
                "position": {
                  "shrink": 0
                },
                "props": {
                  "alignVertical": "top",
                  "style": {
                    "classes": "Global/MarginLeft Global/MarginRight Text/NormalText"
                  },
                  "text": "/",
                  "textStyle": {
                    "fontWeight": "bold",
                    "textAlign": "center"
                  }
                },
                "type": "ia.display.label"
              },
              {
                "meta": {
                  "name": "ScheduleQuantity"
                },
                "propConfig": {
                  "props.text": {
                    "binding": {
                      "config": {
                        "path": "view.custom.SchedTotal"
                      },
                      "transforms": [
                        {
                          "expression": "{value} + \" \" +{view.custom.Units}",
                          "type": "expression"
                        }
                      ],
                      "type": "property"
                    }
                  }
                },
                "props": {
                  "alignVertical": "top",
                  "style": {
                    "classes": "Text/NormalText",
                    "textTransform": "lowercase"
                  },
                  "textStyle": {
                    "fontWeight": "bold",
                    "textAlign": "right"
                  }
                },
                "type": "ia.display.label"
              }
            ],
            "meta": {
              "name": "Metrics"
            },
            "position": {
              "basis": "34px"
            },
            "props": {
              "alignContent": "flex-end",
              "justify": "flex-end"
            },
            "type": "ia.container.flex"
          }
        ],
        "meta": {
          "name": "Body"
        },
        "position": {
          "basis": "168px"
        },
        "props": {
          "direction": "column"
        },
        "type": "ia.container.flex"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column"
    },
    "type": "ia.container.flex"
  }
}