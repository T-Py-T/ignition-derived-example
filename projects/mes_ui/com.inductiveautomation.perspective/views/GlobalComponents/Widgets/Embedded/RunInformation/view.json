                      ],
                      "type": "property"
                    }
                  },
                  "props.textStyle.color": {
                    "binding": {
                      "config": {
                        "expression": "if({view.custom.Pieces}\u003e{view.custom.SchedTotal},\r\n\t\u0027#FF0000\u0027, //Its overrunning (bad)\r\n\t\u0027#FFFFFF\u0027\r\n\t)"
                      },
                      "type": "expr"
                    }
                  }
                },
                "props": {
                  "style": {
                    "classes": "Global/MarginRight Text/NormalText"
                  },
                  "textStyle": {
                    "fontWeight": "bold"
                  }
                },
                "type": "ia.display.label"
              },
              {
                "meta": {
                  "name": "Progress"
                },
                "position": {
                  "grow": 1
                },
                "propConfig": {
                  "props.value": {
                    "binding": {
                      "config": {
                        "path": "view.custom.Percent"
                      },
                      "transforms": [
                        {
                          "expression": "{value} * 100",
                          "type": "expression"
                        }
                      ],
                      "type": "property"
                    }
                  }
                },
                "props": {
                  "style": {
                    "paddingTop": "10px"
                  }
                },
                "type": "ia.display.progress"
              }
            ],
            "meta": {
              "name": "Progress"
            },
            "position": {
              "basis": "43px"
            },
            "props": {
              "style": {
                "classes": "Global/MarginBottom"
              }
            },
            "type": "ia.container.flex"
          },
          {
            "children": [
              {
                "meta": {
                  "name": "CurrentQuantity"
                },
                "position": {
                  "grow": 1,
                  "shrink": 0
                },
                "propConfig": {
                  "props.text": {
                    "binding": {
                      "config": {
                        "path": "view.custom.Pieces"
                      },
                      "transforms": [
                        {
                          "expression": "{value} + \" \" +{view.custom.Units}",
                          "type": "expression"
                        }
                      ],
                      "type": "property"
                    }
                  },
                  "props.textStyle.color": {
                    "binding": {
                      "config": {
                        "expression": "if({view.custom.Pieces}\u003e{view.custom.SchedTotal},\r\n\t\u0027#FF0000\u0027, //Its overrunning (bad)\r\n\t\u0027#FFFFFF\u0027\r\n\t)"
                      },
                      "type": "expr"
                    }
