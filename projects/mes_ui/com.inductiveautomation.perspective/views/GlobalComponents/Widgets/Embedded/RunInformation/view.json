{
  "custom": {
    "Percent": 0.446754,
    "Pieces": "670,131",
    "RunState": "",
    "RunValue": 670131,
    "SchedTotal": "1,500,000",
    "Units": "lbs",
    "WOTotal": "100,000,000"
  },
  "params": {
    "EqPath": "[default]GPA/Wilmington/Mixing/Mixer1",
    "State": ""
  },
  "propConfig": {
    "custom.Percent": {
      "binding": {
        "config": {
          "path": "view.custom.RunValue"
        },
        "transforms": [
          {
            "expression": "min({value} / {view.custom.SchedTotal}, 1)",
            "type": "expression"
          }
        ],
        "type": "property"
      },
      "persistent": true
    },
    "custom.Pieces": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "1": "{view.params.EqPath}"
          },
          "tagPath": "{1}/jobs/currentJob/RunCounts/runGoodParts"
        },
        "transforms": [
          {
            "formatType": "numeric",
            "formatValue": "number",
            "type": "format"
          }
        ],
        "type": "tag"
      },
      "persistent": true
    },
    "custom.RunState": {
      "binding": {
        "config": {
          "expression": "if({view.custom.RunValue} \u003e {view.custom.SchedTotal}, \u0027fail\u0027, \u0027\u0027)"
        },
        "type": "expr"
      },
      "persistent": true
    },
    "custom.RunValue": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "1": "{view.params.EqPath}"
          },
          "tagPath": "{1}/jobs/currentJob/RunCounts/runGoodParts"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "custom.SchedTotal": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "1": "{view.params.EqPath}"
          },
          "tagPath": "{1}/jobs/currentJob/scheduleQuantity"
        },
        "transforms": [
          {
            "formatType": "numeric",
            "formatValue": "number",
            "type": "format"
          }
        ],
        "type": "tag"
      },
      "persistent": true
    },
    "custom.Units": {
      "binding": {
        "config": {
          "path": "session.custom.DisplayUnits"
        },
        "type": "property"
      },
      "persistent": true
    },
    "custom.WOTotal": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "1": "{view.params.EqPath}"
          },
          "tagPath": "{1}/jobs/currentJob/workOrderQuantity"
        },
        "transforms": [
          {
            "formatType": "numeric",
            "formatValue": "number",
            "type": "format"
          }
        ],
        "type": "tag"
      },
      "persistent": true
    },
    "params.EqPath": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.State": {
      "binding": {
        "config": {
          "expression": " if(isNull({view.custom.RunState}), \u0027\u0027, {view.custom.RunState})"
        },
        "type": "expr"
      },
      "paramDirection": "output",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 75,
      "width": 390
    }
  },
  "root": {
    "children": [
      {
        "children": [
          {
            "children": [
              {
                "meta": {
                  "name": "Label"
                },
                "position": {
                  "basis": "50px",
                  "grow": 1
                },
                "propConfig": {
                  "props.text": {
                    "binding": {
                      "config": {
                        "expression": "now()"
                      },
                      "transforms": [
                        {
                          "formatType": "datetime",
                          "formatValue": {
                            "date": "medium",
                            "time": "medium"
                          },
                          "type": "format"
                        }
                      ],
                      "type": "expr"
                    }
                  }
                },
                "props": {
                  "textStyle": {
                    "fontWeight": "bold",
                    "textAlign": "end"
                  }
                },
                "type": "ia.display.label"
              }
            ],
            "meta": {
              "name": "Date"
            },
            "position": {
              "display": false
            },
            "props": {
              "style": {
                "classes": "Global/MarginBottom"
              }
            },
            "type": "ia.container.flex"
          },
          {
            "children": [
              {
                "meta": {
                  "name": "Value"
                },
                "propConfig": {
