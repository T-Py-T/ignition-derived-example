      {
        "events": {
          "dom": {
            "onClick": {
              "config": {
                "script": "\tmodalPath \u003d \u0027GlobalComponents/Modals/Modal\u0027\n\tmodalName \u003d \u0027shiftStart\u0027\n\t\n\tparams \u003d {\n\t\t\u0027Message\u0027: \u0027Shift Started.\u0027,\n\t\t\u0027ModalName\u0027: modalName,\n\t\t\u0027Title\u0027: \u0027Shift Start\u0027\n\t}\n\t\n\tsystem.perspective.openPopup(modalName, modalPath, params\u003dparams, showCloseIcon \u003d False, modal \u003d True, draggable\u003dFalse)"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "StartShift",
          "tooltip": {
            "enabled": true,
            "text": "Start Shift"
          }
        },
        "position": {
          "basis": "39px",
          "display": false
