{
  "custom": {
    "Value": 0
  },
  "params": {
    "State": "value",
    "TagPath": "Newell/Maryville/Assembly/SFN1/",
    "TagProvider": "[default]",
    "Title": "Planned:",
    "Type": "Planned"
  },
  "propConfig": {
    "custom.Value": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "1": "{view.params.TagProvider}",
            "2": "{view.params.TagPath}",
            "3": "{view.params.Type}"
          },
          "tagPath": "{1}{2}/calculatedValues/shiftTime{3}"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "params.State": {
      "paramDirection": "output",
      "persistent": true
    },
    "params.TagPath": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.TagProvider": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.Title": {
      "paramDirection": "inout",
      "persistent": true
    },
    "params.Type": {
      "paramDirection": "inout",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 87,
      "width": 280
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Title"
        },
        "position": {
          "basis": "75px",
          "display": false,
          "grow": 1
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.Title"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "TextFields/TextTitle"
          },
          "textStyle": {
            "textAlign": "start"
          }
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "Value"
        },
        "position": {
          "basis": "32px",
          "grow": 1
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.custom.Value"
              },
              "transforms": [
                {
                  "expression": "runScript(\"gpa.timeRanges.secondsToDateString\", 0, {value})",
                  "type": "expression"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "OverheadScreens/Values"
          },
          "textStyle": {
            "fontSize": "1.5rem",
            "textAlign": "center"
          }
        },
        "type": "ia.display.label"
      }
    ],
    "meta": {
      "name": "root"
    },
    "type": "ia.container.flex"
  }
}