{
  "custom": {},
  "params": {
    "icon": "home"
  },
  "propConfig": {
    "params.icon": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 32,
      "width": 32
    }
  },
  "root": {
    "children": [
      {
        "events": {
          "dom": {
            "onClick": {
              "config": {
                "script": "\tsystem.perspective.sendMessage(messageType\u003d\"icon-selected\", payload\u003d{\"icon\":self.view.params.icon})"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "Icon"
        },
        "position": {
          "basis": "32px",
          "grow": 1
        },
        "propConfig": {
          "props.path": {
            "binding": {
              "config": {
                "expression": "\"material/\" + {view.params.icon}"
              },
              "type": "expr"
            }
          }
        },
        "props": {
          "style": {
            "cursor": "pointer"
          }
        },
        "type": "ia.display.icon"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column"
    },
    "type": "ia.container.flex"
  }
}