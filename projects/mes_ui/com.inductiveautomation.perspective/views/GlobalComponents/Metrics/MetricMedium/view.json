{
  "custom": {},
  "params": {
    "header": "Shift Start",
    "units": "pm",
    "value": "7:00"
  },
  "propConfig": {
    "params.header": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.units": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.value": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 45,
      "width": 113
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "header"
        },
        "position": {
          "basis": "20px",
          "shrink": 0
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.header"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "Text/textColorGray",
            "fontWeight": "bold"
          }
        },
        "type": "ia.display.label"
      },
      {
        "children": [
          {
            "meta": {
              "name": "value"
            },
            "position": {
              "shrink": 0
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "path": "view.params.value"
                  },
                  "type": "property"
                }
              }
            },
            "props": {
              "alignVertical": "bottom",
              "style": {
                "classes": "Text/ValueLarge\nText/textColorGray \n",
                "fontWeight": "bold"
              }
            },
            "type": "ia.display.label"
          },
          {
            "meta": {
              "name": "untis"
            },
            "position": {
              "shrink": 0
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "path": "view.params.units"
                  },
                  "type": "property"
                }
              }
            },
            "props": {
              "alignVertical": "bottom",
              "style": {
                "classes": "Text/LabelTextSmall \nText/textColorGray",
                "fontWeight": "bold",
                "marginLeft": "3px"
              }
            },
            "type": "ia.display.label"
          }
        ],
        "meta": {
          "name": "value"
        },
        "position": {
          "shrink": 0
        },
        "props": {
          "alignItems": "baseline"
        },
        "type": "ia.container.flex"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "overflow": "hidden"
      }
    },
    "type": "ia.container.flex"
  }
}