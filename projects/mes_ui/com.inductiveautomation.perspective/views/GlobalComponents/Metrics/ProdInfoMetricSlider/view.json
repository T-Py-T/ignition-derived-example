{
  "custom": {
    "Value": null,
    "ValueState": null
  },
  "params": {
    "EqPath": "value",
    "Label": "Quality",
    "State": null,
    "TagPath": "",
    "TagProvider": ""
  },
  "propConfig": {
    "custom.Value": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "1": "{view.params.TagProvider}",
            "2": "{view.params.TagPath}",
            "3": "{view.params.Label}"
          },
          "tagPath": "{1}{2}/calculatedValues/oee{3}"
        },
        "transforms": [
          {
            "expression": "{value} * 100",
            "type": "expression"
          }
        ],
        "type": "tag"
      },
      "persistent": true
    },
    "custom.ValueState": {
      "binding": {
        "config": {
          "path": "view.custom.Value"
        },
        "transforms": [
          {
            "code": "\tif value \u003c self.session.custom.PerformanceThresholds.Bad:\n\t\treturn gpa.frontEnd.localUI.Constants.ERROR\n\telif value \u003c self.session.custom.PerformanceThresholds.Warning:\n\t\treturn gpa.frontEnd.localUI.Constants.WARNING\n\telse:\n\t\treturn \"\"",
            "type": "script"
          }
        ],
        "type": "property"
      },
      "persistent": true
    },
    "params.EqPath": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.Label": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.State": {
      "binding": {
        "config": {
          "path": "view.custom.ValueState"
        },
        "transforms": [
          {
            "code": "#\tlabel \u003d self.params.Label\n#\t\n#\tavailabilityThresholds \u003d self.session.custom.AvailabilityThresholds\n#\tperformanceThresholds \u003d self.session.custom.PerformanceThresholds\n#\tqualityThresholds \u003d self.session.custom.QualityThresholds\n#\t\n#\tif label \u003d\u003d \"Performance\":\n#\t\tif value \u003c performanceThresholds.Bad:\n#\t\t\treturn gpa.frontEnd.localUI.Constants.ERROR\n#\t\telif value \u003c performanceThresholds.Warning:\n#\t\t\treturn gpa.frontEnd.localUI.Constants.WARNING\n#\t\telse:\n#\t\t\treturn \"\"\n#\telif label \u003d\u003d \"Availability\":\n#\t\tif value \u003c availabilityThresholds.Bad:\n#\t\t\treturn gpa.frontEnd.localUI.Constants.ERROR\n#\t\telif value \u003c availabilityThresholds.Warning:\n#\t\t\treturn gpa.frontEnd.localUI.Constants.WARNING\n#\t\telse:\n#\t\t\treturn \"\"\n#\telif label \u003d\u003d \"Quality\":\n#\t\tif value \u003c qualityThresholds.Bad:\n#\t\t\treturn gpa.frontEnd.localUI.Constants.ERROR\n#\t\telif value \u003c qualityThresholds.Warning:\n#\t\t\treturn gpa.frontEnd.localUI.Constants.WARNING\n#\t\telse:\n#\t\t\treturn \"\"\n\treturn value",
            "type": "script"
          }
        ],
        "type": "property"
      },
      "paramDirection": "output",
      "persistent": true
    },
    "params.TagPath": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.TagProvider": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
